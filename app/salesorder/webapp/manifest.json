{
  "_version": "1.65.0",
  "sap.app": {
    "id": "salesorder",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.15.4",
      "toolsId": "7676596b-beeb-453c-a829-5313a164a5a3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/salesorder/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.130.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "salesorder.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "A_SalesOrderItemList",
          "target": "A_SalesOrderItemList"
        },
        {
          "pattern": "A_SalesOrderItem({key}):?query:",
          "name": "A_SalesOrderItemObjectPage",
          "target": "A_SalesOrderItemObjectPage"
        }
      ],
      "targets": {
        "A_SalesOrderItemList": {
          "type": "Component",
          "id": "A_SalesOrderItemList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "initialLoad" : "Enabled",
              "contextPath": "/A_SalesOrderItem",
              "variantManagement": "Page",
              "navigation": {
                "A_SalesOrderItem": {
                  "detail": {
                    "route": "A_SalesOrderItemObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "actions": {
                    "fetch": {
                      "press": "salesorder.ext.fetch.fetch.fetch",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "Check Material Stock"
                    }
                  }
                }
              }
            }
          }
        },
        "A_SalesOrderItemObjectPage": {
          "type": "Component",
          "id": "A_SalesOrderItemObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "initialLoad" : "Enabled",
              "editableHeaderContent": false,
              "contextPath": "/A_SalesOrderItem"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
